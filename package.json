  "license": "ISC",
  "bugs": {
    "url": "https://github.com/skanakia/Stockly/issues"
  },
  "homepage": "https://github.com/skanakia/Stockly#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "mongoose": "^5.2.7",
    "morgan": "^1.9.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "path": "^0.12.7"
  }
}
